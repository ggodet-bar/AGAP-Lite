<h1>Listing Process Patterns for Pattern System: </h1>
<h2><%=@pattern_system_name %></h2>

<table>
  <tr>
  </tr>

<% for process_pattern in @process_patterns %>
  <tr>
	<td><%= process_pattern.name %></td>
    <td><%= link_to 'Show', process_pattern %></td>
    <td><%= link_to 'Edit', edit_process_pattern_path(process_pattern) %></td>
    <td><%= link_to 'Destroy', process_pattern, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New process pattern', new_process_pattern_path %><br />

<%= link_to 'Back to pattern systems', :controller => "pattern_systems", :action => "index"%>


